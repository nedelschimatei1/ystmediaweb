# dependencies
/node_modules

.zip

# next.js
/.next/
/out/

# production
/build

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# env files
.env*

# vercel
.vercel

# typescript
*.tsbuildinfo
next-env.d.ts

# -----------------------------------------------------------------------------
# Security / privacy: ignore sensitive data and artifacts
# -----------------------------------------------------------------------------
# Personal data and exports (do NOT commit subscriber lists or PII)
/data/subscribers.json
# any other generated JSON holding private data
data/*.secret.json

# Credentials, keys & certificates
*.pem
*.key
*.p12
*.jks
*.pfx
credentials.json
*.crt
*.cer

# Local IDE / editor and system files
.vscode/
.idea/
.history/
.DS_Store
Thumbs.db

# Temp, caches, coverage
.cache/
tmp/
temp/
uploads/
coverage/
.nyc_output/

# Build / dist artifacts
dist/

# Lint / package manager cache
.eslintcache
.yarn-integrity

# Package manager lockfiles (ignore alternative lockfiles generated locally)
package-lock.json
yarn.lock

# Logs
*.log
npm-debug.log*
.pnpm-debug.log*

# Local function settings (Azure etc.)
local.settings.json

# Optional: ignore local environment files and secrets
.env.local
.env.test
.env.production.local
.env.development.local

# Ignore OS-specific and temporary files
*.swp
*.swo
*.tmp
